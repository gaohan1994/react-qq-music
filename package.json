{
  "name": "react-qq-music",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "prepare": "husky"
  },
  "hooks": {
    "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
    "pre-commit": "git -c core.whitespace=-space-before-tab,-trailing-space --no-pager diff --cached --check && lint-staged",
    "pre-push": "git-branch-is -r \"^(feature|bugfix)-([0-9a-z.-]+)$|^main$\""
  },
  "lint-staged": {
    "./src/**/*.{md,ts,tsx,js,jsx,css,less,scss,html,json,yaml}": "prettier --write"
  },
  "dependencies": {
    "@ant-design/icons": "^5.3.7",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@types/lodash": "^4.17.6",
    "antd": "^5.19.2",
    "autoprefixer": "^10.4.19",
    "classnames": "^2.5.1",
    "husky": "^9.0.11",
    "jotai": "^2.9.0",
    "lint-staged": "^15.2.7",
    "lodash": "^4.17.21",
    "postcss": "^8.4.39",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwind-merge": "^2.4.0",
    "tailwindcss": "^3.4.4",
    "vite-plugin-filewatcher": "^0.1.11",
    "vite-plugin-svg-icons": "^2.0.1",
    "vite-tsconfig-paths": "^4.3.2",
    "wouter": "^3.3.1"
  },
  "devDependencies": {
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "prettier": "^3.3.2",
    "prettier-plugin-organize-imports": "^4.0.0",
    "typescript": "^5.2.2",
    "vite": "^5.3.1"
  },
  "engines": {
    "node": ">=16.0.0",
    "pnpm": "^8.0.0"
  }
}
